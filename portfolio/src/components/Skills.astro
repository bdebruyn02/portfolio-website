---
// Skills section
const skills = [
  "Angular", ".Net Framework", "MS SQL", "Entity Framework", "TypeScript", "Git", "REST APIs", "React", "Python"
];
---
<section class="py-6">
  <h3 class="text-lg font-semibold mb-2">Skills</h3>
  <div class="flex flex-wrap gap-2">
    {skills.map(skill => (
      <span class="bg-gray-700 text-gray-200 px-3 py-1 rounded-full text-xs">{skill}</span>
    ))}
  </div>
</section>
